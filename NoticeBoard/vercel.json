{
    "builds": [{
        "src": "Notices/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxLambdaSize": "15mnb", "runtime": "python3.9"}
    }],

    "routes": [
        {
            "src":"/(.*)",
            "dest": "Notices/wsgi.py"
        }
    ],
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ]
}
